Link:https://www.hackerrank.com/challenges/gem-stones

   static int gemstones(String[] arr) {
            int count=0;
            for(char i='a'; i<='z'; i++)
            {   int c=0;
                for(int j=0; j<arr.length; j++)
                {
                    if(arr[j].contains(String.valueOf(i)))
                    c++;
                }
                if(c==arr.length)
                count++;
            }
            return count;

    }

Link: https://www.hackerrank.com/challenges/library-fine/problem

static int libraryFine(int d2, int m2, int y2, int d1, int m1, int y1) {
            if(y2<y1)
            return 0;

            if(y2-y1!=0)
            return 10000*(y2-y1);

            if(m2<m1)
            return 0;

            else if(m2!=m1)
            return 500*(m2-m1);

            else if(d2>d1)
            return 15*(d2-d1);

            else
            return 0;

    }
